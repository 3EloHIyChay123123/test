local function SLJBZ_fake_script() -- SearchButton.Test 
	local script = Instance.new('Script', SearchButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Textb.Visible = true
		script.Parent.Visible = false
		script.Parent.Parent.Textb.Text = "getting data.."
		local HttpService = game:GetService("HttpService")
	
		local data  = game:HttpGet("https://api.rule34.xxx/index.php?page=dapi&s=post&q=index&tags="..script.Parent.Parent.Tags.Value.."&json=1&limit=1000&pid="..script.Parent.Parent.Page.Value)
		script.Parent.Parent.Textb.Text = "decoding data.."
	
	
		local c
		local success, data = pcall(function()
			c = HttpService:JSONDecode(data)
		end)
		if success then
			script.Parent.Parent.Textb.Text = "creating table.."
			local tabl = {}
			for i, v in pairs(c) do
				if script.Parent.Parent.FileType.Value == ".png" then
					if string.find(v.file_url, ".png") then
						table.insert(tabl, v.id)
						script.Parent.Parent.Textb.Text = "file type is .png.."
					end
				elseif script.Parent.Parent.FileType.Value == ".jpeg" then
					script.Parent.Parent.Textb.Text = "file type is .jpeg.."
					if string.find(v.file_url, ".jpeg") then
						table.insert(tabl, v.id)
					end
				elseif script.Parent.Parent.FileType.Value == ".jpg" then
					script.Parent.Parent.Textb.Text = "file type is .jpg.."
					if string.find(v.file_url, ".jpg") then
						table.insert(tabl, v.id)
					end
				elseif script.Parent.Parent.FileType.Value == ".mp4" then
					script.Parent.Parent.Textb.Text = "file type is .mp4.."
					if string.find(v.file_url, ".mp4") then
						table.insert(tabl, v.id)
					end
				elseif script.Parent.Parent.FileType.Value == ".gif" then
					script.Parent.Parent.Textb.Text = "file type is .gif.."
					if string.find(v.file_url, ".gif") then
						table.insert(tabl, v.id)
					end
				end
			end
			script.Parent.Parent.Textb.Text = "doing random.."
			local image
			if (next(tabl) == nil) then
				script.Parent.Parent.FT.Text = "File Type: "
				script.Parent.Parent.FID.Text = "ID: "
				script.Parent.Parent.O.Text = "Owner: "
				script.Parent.Parent.R.Text = "Rating: "
				script.Parent.Parent.S.Text = "Score: "
				script.Parent.Parent.T.Text = ""
				script.Parent.Parent.St.Text = "Status: "
				script.Parent.Parent.Img.ErrorInf.Text = "Nobody here but us chickens!\n 1. Page you entered doesn't exist\n 2. File type "..script.Parent.Parent.FileType.Value.." not found in this tag or page"
				script.Parent.Parent.Img.ErrorInf.Visible = true
				script.Parent.Parent.Textb.Visible = false
				script.Parent.Parent.Textb.Text = ""
				script.Parent.Visible = true
			else
				image = tabl[math.random(1, #tabl)]
				script.Parent.Parent.Textb.Text = "downloading file.."
				local data2 = game:HttpGet("https://api.rule34.xxx/index.php?page=dapi&s=post&q=index&json=1&id="..image)
				local c2 = HttpService:JSONDecode(data2)
				writefile("Rule34Image"..script.Parent.Parent.FileType.Value, game:HttpGet(c2[1].file_url))
				script.Parent.Parent.Img.ErrorInf.Visible = false
				script.Parent.Parent.Textb.Text = "loading file.."
				if script.Parent.Parent.FileType.Value == ".png" or script.Parent.Parent.FileType.Value == ".jpg" or script.Parent.Parent.FileType.Value == ".jpeg" then
					script.Parent.Parent.Parent.Parent.Parent.Parent.MoreFileInfo.Vid.Visible = false
					script.Parent.Parent.Parent.Parent.Parent.Parent.MoreFileInfo.Img.Visible = true
					script.Parent.Parent.Img.ErrorInf.Visible = false
					script.Parent.Parent.Img.ErrorInf.Text = ""
					script.Parent.Parent.Img.Image = getcustomasset("Rule34Image"..script.Parent.Parent.FileType.Value)
					script.Parent.Parent.Parent.Parent.Parent.Parent.MoreFileInfo.Img.Image = getcustomasset("Rule34Image"..script.Parent.Parent.FileType.Value)
				elseif script.Parent.Parent.FileType.Value == ".mp4" then
					script.Parent.Parent.Parent.Parent.Parent.Parent.MoreFileInfo.Vid.Visible = true
					script.Parent.Parent.Parent.Parent.Parent.Parent.MoreFileInfo.Img.Visible = false
					script.Parent.Parent.Img.ErrorInf.Visible = false
					script.Parent.Parent.Img.ErrorInf.Text = ""
					script.Parent.Parent.Img.Image = ""
					script.Parent.Parent.Parent.Parent.Parent.Parent.MoreFileInfo.Vid.Video = getcustomasset("Rule34Image"..script.Parent.Parent.FileType.Value)
				elseif script.Parent.Parent.FileType.Value == ".gif" then
					script.Parent.Parent.Img.ErrorInf.Text = "I'm sorry, but there is no preview for the gif, but you can take the gif by clicking on this text and copy the link to the gif"
					script.Parent.Parent.Parent.Parent.Parent.Parent.MoreFileInfo.Vid.Video = ""
					script.Parent.Parent.Img.ErrorInf.Visible = true
					script.Parent.Parent.Parent.Parent.Parent.Parent.MoreFileInfo.Vid.Visible = false
					script.Parent.Parent.Parent.Parent.Parent.Parent.MoreFileInfo.Img.Visible = true
					script.Parent.Parent.Img.Image = ""
					script.Parent.Parent.Parent.Parent.Parent.Parent.MoreFileInfo.Img.Image = ""
				end
				script.Parent.Parent.FT.Text = "File Type: "..script.Parent.Parent.FileType.Value
				script.Parent.Parent.FID.Text = "ID: "..c2[1].id
				script.Parent.Parent.O.Text = "Owner: "..c2[1].owner
				script.Parent.Parent.R.Text = "Rating: "..c2[1].rating
				script.Parent.Parent.S.Text = "Score: "..c2[1].score
				script.Parent.Parent.T.Text = c2[1].tags
				script.Parent.Parent.St.Text = "Status: "..c2[1].status
				script.Parent.Parent.Textb.Text = "done."
				script.Parent.Parent.Textb.Visible = false
				script.Parent.Parent.Textb.Text = ""
				script.Parent.Visible = true
			end
		else
			script.Parent.Parent.FT.Text = "File Type: "
			script.Parent.Parent.FID.Text = "ID: "
			script.Parent.Parent.O.Text = "Owner: "
			script.Parent.Parent.R.Text = "Rating: "
			script.Parent.Parent.S.Text = "Score: "
			script.Parent.Parent.T.Text = ""
			script.Parent.Parent.St.Text = "Status: "
			script.Parent.Parent.Img.ErrorInf.Text = "Nobody here but us chickens!\n You entered tag incorrectly or it doesn't exist. Do u mean: "
			script.Parent.Parent.Img.ErrorInf.Visible = true
			script.Parent.Parent.Textb.Visible = false
			script.Parent.Parent.Textb.Text = ""
			script.Parent.Visible = true
		end
	end)
end
coroutine.wrap(SLJBZ_fake_script)()
local function AMZJNY_fake_script() -- SearchButton.Rainbow 
	local script = Instance.new('LocalScript', SearchButton)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	
	counter = 0
	
	while wait(0.1)do
		script.Parent.Main.UIStroke.Color = Color3.fromHSV(zigzag(counter),1,1)
		script.Parent.TextColor3 = Color3.fromHSV(zigzag(counter),1,1)
		counter = counter + 0.01
	end
end
coroutine.wrap(AMZJNY_fake_script)()
local function PMGUDK_fake_script() -- PageTextBox.LocalScript 
	local script = Instance.new('LocalScript', PageTextBox)

	
	
	local textbox = script.Parent
	
	textbox.FocusLost:Connect(function()
		script.Parent.Parent.Parent.RandomPage.Page.Value = textbox.Text
	end)
	
	repeat wait(0.1)
		local t1 = game:HttpGet("https://api.rule34.xxx/index.php?page=dapi&s=post&q=index&tags="..game.Players.LocalPlayer.PlayerGui.Rule34.Main.Main.Pages2.RandomPage.Tags.Value.."&json=0&limit=0")
		t1 = string.sub(t1, 53, 78)
		t1 = t1:gsub('offset="0"', '')
		t1 = t1:gsub('%D+', '')
		t1 = string.sub(t1, 1, 1000)
		t1 = tonumber(t1)
		if t1 >= 1000 then
			textbox.Visible = true
		else
			script.Parent.Parent.Parent.RandomPage.Page.Value= 0
			textbox.Visible = false
		end
	until false
end
coroutine.wrap(PMGUDK_fake_script)()
local function OGAE_fake_script() -- Counter.LocalScript 
	local script = Instance.new('LocalScript', Counter)

	local textbox = script.Parent.Parent.TagsTextBox
	textbox.FocusLost:Connect(function()
		for i = 1,2 do
			local t1 = game:HttpGet("https://api.rule34.xxx/index.php?page=dapi&s=post&q=index&tags="..game.Players.LocalPlayer.PlayerGui.Rule34.Main.Main.Pages2.RandomPage.Tags.Value.."&json=0&limit=0")
			t1 = string.sub(t1, 53, 78)
			t1 = t1:gsub('offset="0"', '')
			t1 = t1:gsub('%D+', '')
			t1 = string.sub(t1, 1, 1000)
			script.Parent.Text = "Tag "..script.Parent.Parent.Tags.Value.." has "..t1.." posts\n"
		end
	end)
end
coroutine.wrap(OGAE_fake_script)()
local function CGNO_fake_script() -- TagsTextBox.LocalScript 
	local script = Instance.new('LocalScript', TagsTextBox)

	local textbox = script.Parent
	
	textbox.FocusLost:Connect(function()
		local s = script.Parent.Text
		s = s:gsub(" ", "%20")
		s = s:gsub("  ", "%20")
		s = s:lower()
		script.Parent.Parent.Tags.Value = s
	end)
end
coroutine.wrap(CGNO_fake_script)()
