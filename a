-- Scripts:

local function ORTNHOX_fake_script() -- Exit.Script 
	local script = Instance.new('Script', Exit)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Vid.Volume = 0
		script.Parent.Parent.Vid.Playing = false
		script.Parent.Parent.Parent.MoreFileInfo.Visible = false
	end)
end
coroutine.wrap(ORTNHOX_fake_script)()
local function TGQFEHS_fake_script() -- MoreFileInfo.Script 
	local script = Instance.new('Script', MoreFileInfo)

	local vid = Instance.new("VideoFrame")
	vid.Looped = true
	vid.Playing = false
	vid.Volume = 0
	vid.Parent = script.Parent.Parent.MoreFileInfo
	vid.Position = UDim2.new(0.186, 0,0.122, 0)
	vid.Size = UDim2.new(0.761, 0,0.749, 0)
	vid.Name = "Vid"
	
	local tags = Instance.new("StringValue")
	tags.Name = "Tags"
	tags.Parent = script.Parent.Parent.Main.Main.Pages2.RandomPage
	tags.Value = ""
	
	local filetype = Instance.new("StringValue")
	filetype.Name = "FileType"
	filetype.Parent = script.Parent.Parent.Main.Main.Pages2.RandomPage
	filetype.Value = ".png"
	
	local filetype = Instance.new("NumberValue")
	filetype.Name = "Page"
	filetype.Parent = script.Parent.Parent.Main.Main.Pages2.RandomPage
	filetype.Value = "0"
	
	
	script:Destroy()
end
coroutine.wrap(TGQFEHS_fake_script)()
local function RSPEPT_fake_script() -- Close.Script 
	local script = Instance.new('Script', Close)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Parent.Parent:Destroy()	
	end)
	
end
coroutine.wrap(RSPEPT_fake_script)()
local function JSIIZA_fake_script() -- RandomPageButton.Script 
	local script = Instance.new('Script', RandomPageButton)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Pages2.RandomPage.Visible = true
		script.Parent.Parent.Parent.Pages2.HelpPage.Visible = true
		script.Parent.Parent.Parent.Sounds.Click:Play()
	end)
	
	script.Parent.MouseEnter:Connect(function()
		script.Parent.TextColor3 = Color3.fromRGB(145, 188, 136)
		script.Parent.Main.UIStroke.Color = Color3.fromRGB(145, 188, 136)
	end)
	
	script.Parent.MouseLeave:Connect(function()
		script.Parent.TextColor3 = Color3.fromRGB(89, 116, 84)
		script.Parent.Main.UIStroke.Color = Color3.fromRGB(89, 116, 84)
	end)
end
coroutine.wrap(JSIIZA_fake_script)()
